# Prob-RAG Environment Configuration
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for OpenAI-based scoring/generation)
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API Key (optional, for Claude-based generation)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Hugging Face Token (optional, for private model access)
HF_TOKEN=your-huggingface-token-here

# Default Configuration
DEFAULT_TAU_LOW=0.3
DEFAULT_TAU_HIGH=0.7
DEFAULT_TOP_K=5
DEFAULT_MODEL=gpt-3.5-turbo

# Experiment Settings
MAX_SAMPLES=1000
RESULTS_DIR=./results
LOG_LEVEL=INFO
